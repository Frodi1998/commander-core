    "eslint": "^8.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdoc": "^3.6.6",
    "mocha": "^9.1.1",
    "prettier": "2.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.8.4"
  }
